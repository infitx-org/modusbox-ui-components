{
  "compilerOptions": {
    "outDir": "./dist",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "lib": ["es2017", "es7", "es6", "dom"],
    "module": "es6",
    "moduleResolution": "node",
    "target": "es2015",
    "jsx": "react",
    "sourceMap": true,
    "declaration": true,
    "allowJs": true
  }
}
